{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "varsIgnorePattern": "^_" }]
  }
}
